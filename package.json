{
  "name": "cf-solver",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "bin": "build/main.js",
  "scripts": {
    "start": "node index.js",
    "dev": "nodemon index.js --ignore clearances.json",
    "webview-build": "cd webview && go build ./webview/main.go",
    "pack": "pkg .",
    "build": "npm-run-all build:*",
    "build:1": "webpack build",
    "build:3": "cross-env NODE_BIN_NAME_OVERRIDE=fetched-v14.20.0-linux-x64:fetched-v14.16.1-win-x64 pkg .",
    "postinstall": "patch-package"
  },
  "type": "module",
  "author": "Maxim Tsialehin",
  "license": "GPL-3.0",
  "dependencies": {
    "chalk": "^5.3.0",
    "get-port": "^7.1.0",
    "http-mitm-proxy": "^1.1.0",
    "https-proxy-agent": "^7.0.4",
    "karma-chrome-launcher": "^3.2.0"
  },
  "devDependencies": {
    "nodemon": "^3.1.3",
    "pkg": "^5.8.1",
    "webpack-cli": "^5.1.4",
    "npm-run-all": "^4.1.5",
    "cross-env": "^7.0.3",
    "patch-package": "^8.0.0"
  },
  "pkg": {
    "scripts": "build/**/*.js",
    "targets": [ "node16-linux", "node16-win"],
    "outputPath": "dist"
  }
}
