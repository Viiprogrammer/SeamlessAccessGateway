{
  "name": "cf-solver",
  "version": "1.0.0",
  "description": "",
  "main": "./src/index.js",
  "bin": "build/index.js",
  "scripts": {
    "start": "node ./src/index.js",
    "dev": "nodemon index.js --ignore clearances.json",
    "webview-build": "cd webview && go build ./webview/main.go",
    "pack": "pkg .",
    "build": "npm-run-all build:*",
    "build:1": "rollup -c",
    "build:3": "cross-env NODE_BIN_NAME_OVERRIDE=fetched-v14.20.0-linux-x64:fetched-v14.16.1-win-x64 pkg .",
    "postinstall": "patch-package"
  },
  "type": "module",
  "author": "Maxim Tsialehin",
  "license": "GPL-3.0",
  "dependencies": {
    "chalk": "^5.3.0",
    "get-port": "^7.1.0",
    "http-mitm-proxy": "^1.1.0",
    "https-proxy-agent": "^7.0.4",
    "karma-chrome-launcher": "^3.2.0",
    "node-abort-controller": "^3.1.1",
    "node-fetch": "^3.3.2"
  },
  "devDependencies": {
    "@rollup/plugin-node-resolve": "^15.2.3",
    "bufferutil": "^4.0.8",
    "cross-env": "^7.0.3",
    "nodemon": "^3.1.3",
    "npm-run-all": "^4.1.5",
    "patch-package": "^8.0.0",
    "pkg": "^5.8.1",
    "rollup": "^4.18.0",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-node-externals": "^7.1.2",
    "utf-8-validate": "^6.0.4"
  },
  "pkg": {
    "scripts": "build/**/*.js",
    "targets": [
      "node16-linux",
      "node16-win"
    ],
    "outputPath": "dist"
  }
}
